        "def par_ou_impar(numero):\n",
        "    if numero % 2 == 0:\n",
        "        return \"Par\"\n",
        "    else:\n",
        "        return \"Ímpar\"\n",
        "numero = int(input(\"Digite um número: \"))\n",
        "resultado = par_ou_impar(numero)\n",
        "print(f\"O número {numero} é {resultado}.\")\n"
